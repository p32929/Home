{
  "manifest_version": 3,
  "name": "p32929-homepage-ext",
  "version": "1.0",
  "description": "A Simple homepage extension created using Shadcn, React, TypeScript, usm-redux etc",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Save this page"
  },
  "permissions": ["activeTab"],
  "author": "p32929",
  "icons": { "16": "home.png", "128": "home.png" },
  "browser_specific_settings": {
    "gecko": {
      "id": "p32929-homepage-ext@p32929.github"
    }
  }
}
